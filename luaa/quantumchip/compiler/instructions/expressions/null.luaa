/*-----------------------------------
    Null
-------------------------------------*/

class QC_Expression_Null : QC_Expression_RawValue {

    function QC_Expression_Null(token) {
        this.Token = token;
    }

    function GetValueType() {
        return "null";
    }
    
    function GetToken() {
        return this.Token;
    }
    
    function GenerateCode(generator, tbl) {
        tbl[#tbl + 1] = "nil";
        
        // TODO: Nullable checking
    }
    
}